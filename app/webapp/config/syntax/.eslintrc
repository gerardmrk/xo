{
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", { "printWidth": 100 }],
    "no-console": "warn",
    "no-extra-boolean-cast": 0,
    "object-literal-sort-keys": false
  }
}
