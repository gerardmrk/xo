  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config=./config/jest/jest.config.js --logHeapUsage --noStackTrace",
    "start": "yarn run start:dev",
    "build": "yarn run build:client",
    "start:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config config/webpack/webpack.config.js  --env.mode=development --env.source=client",
    "build:client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config config/webpack/webpack.config.js --env.mode=production --env.source=client",
    "build:renderer": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config config/webpack/webpack.config.js --env.mode=production --env.source=renderer",
    "test:withcov": "cross-env NODE_ENV=test jest --config=./config/jest/jest.config.js --coverage",
    "i18n:sync": "node ./scripts/update-translations/index.js",
    "proto:gen:js": "./node_modules/.bin/pbjs -t static-module -w commonjs -o ./src/renderer/engine/proto/index.js ./src/renderer.proto; ./node_modules/.bin/pbts -o ./src/renderer/engine/proto/index.d.ts ./src/renderer/engine/proto/index.js",
    "proto:gen:go": "protoc --go_out=./src/server/internal ./src/renderer.proto",
    "validate:settings": "node ./scripts/validate-settings/index.js",
    "tmp:rserver": "cross-env DEBUG=* node ./dist/renderer/renderer.js --mode uds --addr /tmp/server.sock",
    "tmp:rclient": "node ./tmp.rclient.js"
  },
